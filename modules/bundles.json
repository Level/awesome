{
  "title": "Bundles",
  "description": [
    "Convenience modules that bundle [a store](#stores) with [levelup] and [encoding-down]."
  ],
  "compatibility": [{
    "package": "levelup",
    "via": "level-packager"
  }],
  "modules": {
    "level": {
      "github": "Level/level",
      "description": [
        "Bundle for [leveldown].",
        "Main entry point for beginners."
      ],
      "copyrightYear": 2013,
      "author": "Rod Vagg"
    },
    "level-mem": {
      "github": "Level/level-mem",
      "description": "Bundle for [memdown].",
      "copyrightYear": 2012,
      "author": null
    },
    "level-rocksdb": {
      "github": "Level/level-rocksdb",
      "description": "Bundle for [rocksdb].",
      "copyrightYear": 2013,
      "author": null
    },
    "level-browserify": {
      "github": "Level/level-browserify",
      "description": [
        "Bundle for [level-js] and [leveldown].",
        "Will soon be superseded by the [level] bundle."
      ],
      "copyrightYear": 2012,
      "author": null
    },
    "level-hyper": {
      "github": "Level/level-hyper",
      "description": "Bundle for [leveldown-hyper].",
      "copyrightYear": 2012,
      "author": null
    },
    "level-indexed": {
      "github": "kapetan/level-indexed",
      "description": [
        "Bundle for [indexeddown].",
        "Alternative to [level-browserify] and [level]."
      ]
    }
  }
}
